    "github.gitProtocol": "ssh",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "stylelint.packageManager": "yarn",
    "npm.packageManager": "yarn",
    "eslint.packageManager": "yarn",
    "errorLens.lintFilePaths": {
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
