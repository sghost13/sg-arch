        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ]
    },
    "editor.fontSize": 12,
    "shellcheck.enableQuickFix": false,
    "sonarlint.rules": {
        "css:S4662": {
            "level": "off"
        }
